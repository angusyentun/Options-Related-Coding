tandard Error: 0.03291344632776926\n","MCV Option Price: 10.456746430936313\n","MCV Standard Error: 0.016567238243551604\n","Closed-form: 10.450583572185565\n"]}],"source":["# Input variables and parameters for Basic Monte Carlo\n","Nrepl = 200000  # Total number of simulations\n","Nstep = 256  # Total number of time discretization\n","\n","# Input contract variables and model parameters\n","s0 = 100\n","K = 100\n","T = 1\n","r = 0.05\n","sig = 0.20\n","\n","\n","dt = T / Nstep  # Time step size\n","w = np.sqrt(dt) * np.random.randn(Nrepl, 2 * Nstep)  # Samples of Brownian motion increments\n","\n","# Basic Monte Carlo\n","s = np.ones((Nrepl, Nstep + 1)) * s0\n","for l in range(1, Nstep + 1):\n","    s[:, l] = s[:, l - 1] * np.exp((r - (1 / 2) * (sig ** 2)) * dt + sig * w[:, l - 1])\n","\n","DP_MC = np.exp(-r * T) * np.maximum(s[:, -1] - K, 0)  # Discounted payoff samples\n","Price_MC = np.mean(DP_MC)  # Sample mean\n","SE_MC = np.sqrt(np.var(DP_MC) / Nrepl)  # Sample standard error\n","\n","# Martingale Control Variate Method\n","t_control = np.arange(Nstep, 0, -1) * dt\n","t_control = np.tile(t_control, (Nrepl, 1))\n","delta = bs_delta(s[:, :Nstep], K, T, r, sig) # bs_delta is a function to compute deltas under the BS model\n","discount = np.exp(-r * dt * np.arange(Nstep))  # Discount factors\n","discount = np.tile(discount, (Nrepl, 1))\n","control = np.sum(discount * delta * s[:, :Nstep] * sig * w[:, :Nstep], axis=1) #delta hedging strategy\n","\n","Cov = np.cov(DP_MC, control)\n","lambda_val = Cov[0, 1] / (np.var(control))  # Optimal control parameter\n","\n","DP_MCV = DP_MC - lambda_val * control\n","Price_MCV = np.mean(DP_MCV)  # Option price estimator\n","SE_MCV = np.sqrt(np.var(DP_MCV) / Nrepl)\n","\n","\n","# Plotting first 1000 discounted payoffs\n","plt.figure(figsize=(7, 3))\n","plt.plot(np.arange(1, 5001), DP_MC[:5000], 'blue', label='Basic Monte Carlo')\n","plt.plot(np.arange(1, 5001), DP_MCV[:5000], 'red', label='Martingale Control Variate')\n","plt.xlabel('Number of simulations')\n","plt.ylabel('Discounted payoff')\n","plt.legend()\n","plt.show()\n","print(\"BMC Option Price:\", Price_MC)\n","print(\"BMC Standard Error:\", SE_MC)\n","print(\"MCV Option Price:\", Price_MCV)\n","print(\"MCV Standard Error:\", SE_MCV)\n","print(\"Closed-form:\", bsm_call(s0, K, T, r, sig))"]}],"metadata":{"kernelspec":{"display_name":"Python 3","name":"python3"},"language_info":{"name":"python"},"colab":{"provenance":[{"file_id":"1LG0exNPi8YckaYGGuCTn7Z5R1QzTpRFX","timestamp":1695554339041},{"file_id":"1Hq7-JlQ9119wGUU3Npj0NXpQF8LgPo1e","timestamp":1692103737402}]}},"nbformat":4,"nbformat_minor":5}